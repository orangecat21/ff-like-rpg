    "build:dev": "rm -rf dist && env NODE_ENV=development webpack",
    "build": "rm -rf dist && env NODE_ENV=production webpack",
    "lint": "eslint src/**/*.js",
    "prettier": "prettier --write .",
    "dev": "env NODE_ENV=development webpack server",
